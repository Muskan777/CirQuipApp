{
  "expo": {
    "name": "CirQuip",
    "slug": "CirQuip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/asset1.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "notification": {
      "icon": "./assets/notif.png",
      "color": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cirquip.cirquip"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "package": "com.cirquip.cirquip",
      "versionCode": 1
    }
  }
}
